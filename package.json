{
  "name": "@connectstrata/strata-frontend-sdk",
  "version": "1.0.0",
  "description": "Frontend sdk for simplifying integration auth",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
